@model MVCjoin.Models.RegisterList
@using System.Data;
@{
    ViewBag.Title = "Details";
}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<main>
    <table class="table">
        <thead>
            <tr>
                @foreach (DataColumn dc in Model.Rtable.Columns)
                {
                    <td scope="col">
                        @dc.ColumnName
                    </td>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (DataRow dr in Model.Rtable.Rows)
            {
                <tr>
                    @foreach (DataColumn dc in Model.Rtable.Columns)
                    {
                        <td>
                            @dr[dc.ColumnName]
                        </td>
                    }
                </tr>
            }
        </tbody>
    </table>
</main>

