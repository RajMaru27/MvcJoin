@model MVCjoin.Models.ExamDetails
@using System.Data;
@{
    ViewBag.Title = "Index";
}


@using (Html.BeginForm("AddForm", "Exam", FormMethod.Post))
{
    @Html.LabelFor(x => x.SubjectId)
    @Html.DropDownListFor(x => x.SubjectId,Model.sublist)
    <br />
    @Html.LabelFor(x => x.RegisterId)
    @Html.DropDownListFor(x => x.RegisterId,Model.reglist)
    <br />
    @Html.LabelFor(x => x.marks)
    @Html.TextBoxFor(x => x.marks)
    <br />
    @Html.LabelFor(x => x.OutOf)
    @Html.TextBoxFor(x => x.OutOf)
    <br />
    <input type="submit"value="submit" />
}
